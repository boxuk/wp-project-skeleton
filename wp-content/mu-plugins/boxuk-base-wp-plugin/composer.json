{
    "name": "boxuk/boxuk-base-wp-plugin",
    "homepage": "https://boxuk.com",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist",
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "BoxUk\\Plugins\\Base\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoxUk\\Plugins\\Base\\Tests\\": "tests"
        }
    },
    "require": {
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "roots/wordpress": "^5",
        "symfony/dotenv": "^5.0",
        "wp-phpunit/wp-phpunit": "^5.4"
    },
    "suggest": {
        "timber/timber": "Needed for the i18n-twig WP CLI command.",
        "twig/twig": "Needed for the i18n-twig WP CLI command.",
        "twig/extensions": "Needed for the i18n-twig WP CLI command."
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
